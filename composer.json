{
    "require": {
        "symfony/http-foundation": "^5.4",
        "twig/twig": "~1.4"
    },
    "config": {"vendor-dir": "src/Vendor"},
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
     "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
     "autoload-dev": {
        "psr-4": {
            "App\\": "src/",
            "Tests\\" : "Tests"
        }
    }
}
